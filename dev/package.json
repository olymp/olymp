{
  "version": "11.0.1-2",
  "name": "olymp-dev",
  "description": "A starter kit giving you the minimum requirements for a production ready universal react application.",
  "main": "./src2/index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "clean": "node tools/scripts/clean",
    "development": "node tools/development",
    "build": "node tools/scripts/build",
    "analyze": "node tools/scripts/analyze",
    "start": "node ./build/server",
    "deploy": "node tools/scripts/deploy",
    "lint": "eslint src",
    "flow": "node tools/scripts/flow",
    "flow:defs": "flow-typed install -f 0.34 --overwrite",
    "flow:report": "flow-coverage-report -i 'src/**/*.js' -t html -t json -t text",
    "flow:remove": "node tools/scripts/removeTypes.js && rimraf tools/scripts/removeTypes.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ctrlplusb/react-universally.git"
  },
  "keywords": [
    "react",
    "boilerplate",
    "starter kit",
    "universal",
    "javascript",
    "express",
    "webpack"
  ],
  "author": "Sean Matheson <sean@ctrlplusb.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ctrlplusb/react-universally/issues"
  },
  "homepage": "https://github.com/ctrlplusb/react-universally#readme",
  "dependencies": {
    "assets-webpack-plugin": "3.5.0",
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-import": "1.1.0",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-plugin-transform-class-properties": "6.19.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-destructuring": "6.19.0",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "babel-plugin-transform-react-jsx-self": "6.11.0",
    "babel-plugin-transform-react-jsx-source": "6.9.0",
    "babel-preset-env": "1.1.4",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-3": "6.17.0",
    "babel-template": "6.16.0",
    "chokidar": "1.6.1",
    "css-loader": "0.26.1",
    "empty-loader": "1.0.1",
    "eslint": "3.12.2",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-flowtype": "2.29.2",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "flow-bin": "0.37.4",
    "flow-coverage-report": "0.2.0",
    "flow-remove-types": "1.1.2",
    "flow-typed": "2.0.0",
    "glob": "7.1.1",
    "happypack": "3.0.2",
    "html-webpack-plugin": "2.24.1",
    "less": "2.7.2",
    "less-loader": "2.2.3",
    "md5": "2.2.1",
    "node-notifier": "4.6.1",
    "react-hot-loader": "3.0.0-beta.6",
    "regenerator-runtime": "0.10.1",
    "rimraf": "2.5.4",
    "semver": "5.3.0",
    "style-loader": "0.13.1",
    "webpack": "2.2.0-rc.3",
    "webpack-bundle-analyzer": "2.2.1",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.14.0",
    "webpack-md5-hash": "0.0.5",
