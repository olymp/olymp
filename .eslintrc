{
  "parser": "babel-eslint",
  "extends": [
    "prettier",
    "prettier/react",
    "airbnb"
  ],
  "plugins": [
    "react",
    "prettier"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-duplicate-imports": 0,
    "react/jsx-filename-extension": 0,
    "react/sort-comp": 0,
    "global-require": 0,
    "comma-dangle": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "curly": ["error", "all"]
  }
}

